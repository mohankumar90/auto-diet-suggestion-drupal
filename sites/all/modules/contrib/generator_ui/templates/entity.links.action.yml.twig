# All action links for this module
{{ entity_name }}.add_form:
# Which route will be called by the link
  route_name: {{ entity_name }}.add_form
  title: 'Add {{ entity_class }}'
# Where will the link appear, defined by route name.
  appears_on:
    - entity.{{ entity_name }}.collection
    - entity.{{ entity_name }}.canonical

