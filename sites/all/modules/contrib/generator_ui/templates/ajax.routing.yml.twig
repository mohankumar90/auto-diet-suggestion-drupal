{%  if choice == "modal" or choice == "dialog" %}
{{ module_name }}.ajax_link:
  path: '/example/ajax-link'
  defaults:
    _controller: '\Drupal\{{ module_name }}\Controller\{{ controller }}::ajaxLink'
  requirements:
    _access: 'TRUE'
{% else %}
{{ module_name }}.ajaxlink:
  path: '/example/ajaxlink/{nid}'
  defaults:
    _controller: '\Drupal\{{ module_name }}\Controller\{{ controller }}::ajaxLink'
  requirements:
    _permission: 'access content'
{{ module_name }}.change_status:
  path: '/example/status/{nid}'
  defaults:
    _controller: '\Drupal\{{ module_name }}\Controller\{{ controller }}::changeStatusContent'
  requirements:
    _permission: 'access content'
{% endif %}
