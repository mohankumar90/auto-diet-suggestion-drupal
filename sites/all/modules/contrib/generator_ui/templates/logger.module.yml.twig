<?php

/**
 * @file
 * Contains {{ module_name }}.module.
 */
/**
 *  Implements hook_cron().
 */
function {{ module_name }}_cron() {
  $user = \Drupal::currentUser();
  \Drupal::logger('{{ module_name }}')
    ->{{ method_watchdog }}('User %user is connected at %time ', array('%user' => $user->getUsername(),'%time'=>date_iso8601(REQUEST_TIME)));
}