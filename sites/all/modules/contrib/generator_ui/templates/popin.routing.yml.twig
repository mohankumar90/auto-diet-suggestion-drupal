{{ module_name }}.popin:
  path: '/generators-modals'
  defaults:
    _controller: '\Drupal\{{ module_name }}\Controller\{{ controller }}::exampleModals'
    _title: 'Page Popin'
  requirements:
    _permission: 'access content'

{{ module_name }}.popinform:
  path: '/popin-form'
  defaults:
    _form: '\Drupal\{{ module_name }}\Form\{{ form }}'
    _title: 'Popin Form'
  requirements:
    _permission: 'access content'